#include<stdio.h>

int main()
{
	int vetor[4][4], i, j;
	
	for(i=0; i<3; i++)
	{
		for(j=0; j<3; j++)
		{
			printf("Digite o valor para posicao %d:%d da matriz: ", i, j);
			scanf("%d", &vetor[i][j]);
		}
	}
	
	for(i=0; i<3; i++)
	{
		for(j=0; j<3; j++)
		{
			printf(" %d ", vetor[i][j]);
		}
		printf("\n");
	}
}
